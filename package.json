{
  "name": "swipe_and_shop",
  "version": "1.0.0",
  "description": "An app to buy and sell items using a Tinder style selection process",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently -n 'server,client' -c 'red,green' \"nodemon server.js\" \"npm run client\"",
    "start:test": "concurrently -n 'server,client' -c 'red,green' \"PORT=3030 MONGODB_URI='mongodb://localhost/swipe_and_shop_test' npm start\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GarethWoodman/swipe_and_shop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GarethWoodman/swipe_and_shop/issues"
  },
  "homepage": "https://github.com/GarethWoodman/swipe_and_shop#readme",
  "dependencies": {
    "axios": "^0.19.2",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.0",
    "mongoose": "5.9.29",
    "morgan": "^1.10.0"
  }
}
